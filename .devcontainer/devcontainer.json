{
    "name": "hw1",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "customizations": {
        "vscode": {
            "extensions": [
                "octref.vetur",
                "dbaeumer.vscode-eslint",
                "stylelint.vscode-stylelint"
            ]
        }
    },
    "workspaceFolder": "/app",
    "mounts": [
        "source=${localWorkspaceFolder},target=/app,type=bind"
    ],
    "remoteUser": "root"
}